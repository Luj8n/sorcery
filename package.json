    "tailwindcss": "2.1.2",
    "zod": "3.9.8"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.30",
    "eslint": "7.32.0",
    "husky": "6.0.0",
