    "blitz": "0.43.0",
    "final-form": "4.20.4",
    "next-themes": "0.0.15",
    "postcss": "8.4.3",
    "react": "18.0.0-beta-149b420f6-20211119",
    "react-dom": "18.0.0-beta-149b420f6-20211119",
    "react-final-form": "6.5.7",
